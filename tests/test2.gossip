-- assign
(setq step 100)

(print step)

-- function
(defun add (a b c)
    (+ a (+ b c)))

(defun increment (a)
    (+ a step))

(print
    (increment (add 1 2 3)))

-- gt
(print (> 2 1))

-- lt
(print (< 2 1))

-- eq
(print (= 2 1))

-- cond
(cond ((> step 99) (print 1))
      ((> step 101) (print 2)))