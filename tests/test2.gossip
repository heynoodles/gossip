-- assign
(setq step 100)

(print step)

-- function
(define (add a b c)
    (+ a (+ b c)))

(define (increment a)
    (+ a step))

(print
    (increment (add 1 2 3)))

-- gt
(print (> 2 1))

-- lt
(print (< 2 1))

-- eq
(print (= 2 1))

-- cond
(cond ((> step 99) (print "greater than 99"))
      ((> step 101) (print "greater than 101")))